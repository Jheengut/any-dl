parsername "ard_mediathek_get": ("http://www.ardmediathek.de", "http://mediathek.daserste.de")
start
  match "(rtmpt{0,1}://[^\"]+).*?(mp4:[^\"]+)\"";
  print_match;
  rowselect(0);
  print_string("------------------------");
  print();
end



parsername "ndr_mediathek_get": ( "http://www.ndr.de" )
start
  match "http://.*?mp4";
  print();
end









